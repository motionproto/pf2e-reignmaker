{
  "name": "pf2e-reignmaker",
  "version": "1.0.0",
  "description": "Kingdom Building management system for Pathfinder 2e",
  "license": "MIT",
  "private": true,
  "type": "module",
  "author": "Mark Pearce & Anthropic Claude",
  "contributors": [
    "Mark Pearce & Anthropic Claude"
  ],
  "imports": {
    "#runtime/*": "@typhonjs-fvtt/runtime/*",
    "#standard/*": "@typhonjs-fvtt/standard/*"
  },
  "dependencies": {
    "@typhonjs-fvtt/runtime": "^0.3.0-next.3",
    "@typhonjs-fvtt/standard": "^0.3.0-next.3",
    "chart.js": "^4.5.1",
    "svelte": "^4.2.20"
  },
  "devDependencies": {
    "@nivo/bar": "^0.99.0",
    "@nivo/core": "^0.99.0",
    "@nivo/line": "^0.99.0",
    "@nivo/pie": "^0.99.0",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "foundry-pf2e": "github:7H3LaughingMan/foundry-pf2e",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "svelte-check": "^4.0.9",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.8.1",
    "tsx": "^4.7.0",
    "typescript": "^5.9.2",
    "vite": "^5.4.11",
    "vite-plugin-static-copy": "^3.1.2"
  },
  "browserslist": [
    ">5%",
    "not IE 11"
  ],
  "scripts": {
    "build": "node buildscripts/run-python.js buildscripts/combine-data.py && node buildscripts/run-python.js buildscripts/generate-types.py && vite build",
    "generate-types": "node buildscripts/run-python.js buildscripts/generate-types.py",
    "dev": "node buildscripts/run-python.js buildscripts/combine-data.py && node buildscripts/run-python.js buildscripts/generate-types.py && vite --config vite.config.dev.ts",
    "dev:setup": "node buildscripts/setup-dev.js",
    "dev:start": "npm run dev:setup && npm run dev",
    "dev:proxy": "vite --config vite.config.dev.ts",
    "dev:standalone": "vite --config vite.config.dev.ts",
    "clean": "node -e \"require('fs').rmSync('dist', { recursive: true, force: true })\"",
    "deploy": "npm run clean && node buildscripts/deploy.js",
    "deploy:dev": "npm run dev:setup",
    "package": "node buildscripts/package.js",
    "preview": "vite preview",
    "proxy": "npm run dev",
    "check-migration": "node buildscripts/check-action-migration.cjs",
    "check-migration:verbose": "node buildscripts/check-action-migration.cjs --verbose",
    "test-pipeline": "tsx src/tests/pipeline-coordinator.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/motionproto/pf2e-reignmaker.git"
  },
  "bugs": {
    "url": "https://github.com/motionproto/pf2e-reignmaker/issues"
  },
  "homepage": "https://github.com/motionproto/pf2e-reignmaker#readme"
}
